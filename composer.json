{
    "name": "vendor/armenian-payments",
    "description": "Armenian payment methods in one place",
    "keywords": [
        "vendor",
        "armenian-payments"
    ],
    "homepage": "https://github.com/vendor/armenian-payments",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ABN",
            "email": "ashbarseghyan99@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\ArmenianPayments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\ArmenianPayments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\ArmenianPayments\\ArmenianPaymentsServiceProvider"
            ],
            "aliases": {
                "ArmenianPayments": "Vendor\\ArmenianPayments\\ArmenianPaymentsFacade"
            }
        }
    }
}
